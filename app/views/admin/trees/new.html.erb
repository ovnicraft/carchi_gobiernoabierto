
<%= form_for(@tree, :url => admin_trees_path) do |f| %>
  <%= f.error_messages %>

  <p>
    <label for="tree_name_es">Nombre</label><br />
    <%= f.text_field :name_es %>
  </p>
  <p>
    <label for="tree_name_eu">Izena</label><br />
    <%= f.text_field :name_eu %>
  </p>
  <p>
    <label for="tree_name_en">Name</label><br />
    <%= f.text_field :name_en %>
  </p>
  
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_trees_path %>
