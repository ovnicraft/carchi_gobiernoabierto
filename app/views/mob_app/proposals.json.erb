<%- 
  proposals = []
  @proposals.each do |proposal|
    proposals << render(:partial => 'mob_app/cells/proposal_cell.json', 
                        :locals => {:proposal => proposal, :sort_id => (proposal.published_at.to_i*10000 + proposal.id), :target => :list})
  end
-%>

{
  "items": [
    <%=raw proposals.join(", \n") %>
  ], 
  "has_older" : false
}
