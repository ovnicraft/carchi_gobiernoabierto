<%- 
  photos = []
  @photos.each do |p|
    photos << render(:partial => 'mob_app/cells/photo_cell.json', :locals => {:photo => p, :url => p.version(:n320), :width =>  (photo_width(p, :n320) || 320), :height => (photo_height(p, :n320) || 214)})
  end
-%>

{
 "thumbs": [
   <%=raw photos.join(", \n") %>
 ], 
 "has_older" : <%= !@document.present? %>
}
